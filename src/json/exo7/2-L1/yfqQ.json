    },
    {
      "id": "caaca97b-b72e-453b-817c-8e56e3fa24aa",
      "type": "question",
      "value": {
        "latex": "$A \\subset B \\Rightarrow \\sup A \\leqslant \\sup B$,",
        "html": "<p><span class=\"math inline\">\\(A \\subset B \\Rightarrow \\sup A \\leqslant\n\\sup B\\)</span>,</p>\n"
      }
