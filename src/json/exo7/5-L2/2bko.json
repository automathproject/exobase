    {
      "id": "154ee5fe-b7c7-4f96-a47f-44af1a657e5f",
      "type": "reponse",
      "value": {
        "latex": "$x=-1  \\Rightarrow $ cv (série alternée), $x=1  \\Rightarrow $ dv.",
        "html": "<p><span class=\"math inline\">\\(x=-1  \\Rightarrow\\)</span> cv (série\nalternée), <span class=\"math inline\">\\(x=1  \\Rightarrow\\)</span> dv.</p>\n"
